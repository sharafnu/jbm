	<queryString>
		<![CDATA[SELECT
     app."appointment_no",
     app."appointment_date",
     app."start_date",
     app."end_date",
     cus."first_name",
     cus."last_name",
     cus."mobile_1",
     cus."mobile_2",
     ca."address_type",
     ca."flat_no",
     ca."building_name",
     ca."remarks"
FROM
     "appointment" app INNER JOIN "customer" cus ON app."customer_id" = cus."id"
     INNER JOIN "employee" emp ON app."employee_id" = emp."id"
     INNER JOIN "customer_address" ca ON app."customer_address_id" = ca."id"
where app.employee_id=$P{staffId} and to_char(app.appointment_date, 'DD-MM-YYYY') between
$P{startDate} and $P{endDate} and app."appointment_status" <> 'Cancelled']]>
	</queryString>
